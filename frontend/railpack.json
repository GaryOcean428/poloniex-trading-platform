{
  "$schema": "https://schema.railpack.com",
  "version": "1",
  "packages": {
    "node": "20",
    "yarn": "4.9.2"
  },
  "install": {
    "commands": [
      "corepack enable",
      "corepack prepare yarn@4.9.2 --activate",
      "yarn --cwd .. install --check-cache"
    ]
  },
  "build": {
    "commands": [
      "yarn run build"
    ]
  },
  "deploy": {
    "command": "node serve.js"
  }
}
