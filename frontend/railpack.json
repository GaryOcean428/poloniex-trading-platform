{
  "$schema": "https://schema.railpack.com",
  "provider": "railway",
  "version": "1",
  "packages": {
    "node": "20",
    "yarn": "4.9.2"
  },
  "install": {
    "commands": [
      "cd .. && yarn install --check-cache"
    ]
  },
  "build": {
    "commands": [
      "cd .. && yarn workspace frontend build"
    ]
  },
  "deploy": {
    "command": "node serve.js"
  }
}
