{
  "$schema": "https://schema.railpack.com",
  "provider": "node",
  "packageManager": "npm",
  "node": {
    "version": "22"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "install": {
    "commands": [
      "npm install"
    ]
  },
  "build": {
    "commands": [
      "npm run build:railway"
    ]
  },
  "start": {
    "command": "node serve.js"
  },
  "health": {
    "path": "/healthz",
    "timeout": 300
  }
}
