{
  "$schema": "https://schema.railpack.com",
  "build": {
    "steps": {
      "install": {
        "commands": ["corepack enable", "corepack prepare yarn@4.9.2 --activate", "yarn install --immutable"]
      },
      "build": {
        "commands": ["yarn build"]
      }
    }
  },
  "deploy": {
    "startCommand": "yarn start",
    "healthCheckPath": "/api/health",
    "healthCheckTimeout": 300
  }
}