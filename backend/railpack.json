{
  "$schema": "https://schema.railpack.com",
  "build": {
    "steps": [
      {
        "name": "install",
        "command": "yarn install --immutable"
      },
      {
        "name": "build",
        "command": "yarn build"
      }
    ]
  },
  "deploy": {
    "startCommand": "node dist/index.js"
  }
}
